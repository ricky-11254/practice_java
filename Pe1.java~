import java.util.Scanner;
class Pe1{
	public static void main(String args[]){
	Scanner input = new Scanner();
	int T;
	T = input.nextInt;
	int sum = 0;
	while(T--){
		int N;
		T = input.nextInt;
		for(int i =0;i<=N;i++){
			if(1%3==0 || i%5==0){
				sum+=i;
			}
		}
		System.out.println(sum);
		}
	}
}

